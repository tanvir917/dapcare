
@{
    ViewBag.Title = "ViewOrderPdf";
}


<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th> Invoice Number </th>
        <th>TotalBill</th>
        <th>Date Time</th>

        <th>EmployeName</th>
        <th>Phone Number</th>
        <th>Locations</th>
        <th>Action</th>
    </tr>
    @{
    
    int count=1;
    }
    @for (int i = 0; i < Model.Rows.Count; i++)
    {

        <tr>
            <td> @count</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][5]</td>
            <td>@Model.Rows[i][6]</td>
            <td>@Model.Rows[i][7]</td>

            <td><a href="~/Admin/Downloadpdf/@(Model.Rows[i][1])">Download</a></td>
        </tr>
        count = count + 1;
    }

</table>