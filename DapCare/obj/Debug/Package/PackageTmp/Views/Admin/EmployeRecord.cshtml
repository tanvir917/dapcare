
@{
    ViewBag.Title = "EmployeRecord";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>Employe First Name </th>
        <th>Employe Last Name</th>
        <th>SubcategoryName</th>
        <th>Action</th>
        <th>Action</th>
    </tr>
    @{  int count = 1;
        }
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
       
        <tr>
            <td> @count</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td><a href="@Url.Action("EditEmploye","Admin", new { EmployeId =@Model.Rows[i][0]})">Edit</a></td>
            @*<td><a href="/MyAdmin/Edit/@(Model.Rows[i][0])">Edit</a></td>*@
            <td><a href="~/Admin/DeleteEmploye/@(Model.Rows[i][0])">Delete</a></td>
        </tr>
        count++;
    }

</table>


