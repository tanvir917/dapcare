
@{
    ViewBag.Title = "MyProducts";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>First Name </th>
        <th>Last Name</th>
        <th>Phone Number</th>
        <th>Area</th>
        <th>Action</th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        int count = 1;
        <tr>
            <td> @count</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][5]</td>
            <td><a href="~/Admin/ChangeEmploye/@(Model.Rows[i][0])">Change</a></td>
        </tr>
        count++;
    }

</table>
<div class="btnbtn-success">
    @Html.ActionLink("Create Pdf", "CreatePdf", "Admin")
</div>

<button type="button" class="btn-success" value="Next" onclick="location.href='@Url.Action( "ViewOrderDetails","Admin")'">Next</button>

