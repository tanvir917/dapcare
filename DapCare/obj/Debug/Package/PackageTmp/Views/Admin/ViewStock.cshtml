
@{
    ViewBag.Title = "ViewStock";
}


<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>Product Name </th>
        <th>Pack Name</th>
        <th>Quantity </th>
     
        <th>Action</th>
        <th>Action</th>
    </tr>

    @{ 


        int count = 1;
    }
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        
        <tr>
            <td> @count</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][4]</td>
            <td>@Model.Rows[i][5]</td>
            <td><a href="~/Admin/UpdateStock/@(Model.Rows[i][0])">Change</a></td>
    <td><a href="~/Admin/Deletestocks/@(Model.Rows[i][0])">Delete</a></td>
        </tr>
     count++;
    }

</table>
