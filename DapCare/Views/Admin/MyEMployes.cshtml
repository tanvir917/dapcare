
@{
    ViewBag.Title = "MyProducts";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>Product Name </th>
        <th>Quanity</th>
        <th>SubcategoryName</th>
        <th>Action</th>
        <th>Action</th>
    </tr>
    @{


        int count = 1;
    }

    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td> @count</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td><a href="@Url.Action("Edit","Admin", new { @ProductId =@Model.Rows[i][0]})">Edit</a></td>
            @*<td><a href="/MyAdmin/Edit/@(Model.Rows[i][0])">Edit</a></td>*@
            <td><a href="~/Admin/Delete/@(Model.Rows[i][0])">Delete</a></td>
        </tr>
        count++;
    }

</table>

