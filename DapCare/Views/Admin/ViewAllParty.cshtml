
@{
    ViewBag.Title = "ViewAllParty";
}


<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>Party Name </th>
        <th>Phone Number</th>
        <th>Address</th>
        <th>EmployeName</th>
        <th>Action</th>

    </tr>

    @{


        int count = 1;
    }

    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@count</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][5]</td>
            <td><a href="@Url.Action("EditParty","Admin", new { PartyId  =@Model.Rows[i][0]})">Edit</a></td>
            @*<td><a href="/MyAdmin/Edit/@(Model.Rows[i][0])">Edit</a></td>*@

        </tr>

        count++;
    }

</table>
