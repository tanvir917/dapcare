
@{
    ViewBag.Title = "ShowDeposit";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>EmployeName </th>
        <th>ParyName</th>
        <th>Dates</th>
        <th>Cash</th>
    </tr>

    @{
       int count = 1;
    }

    @for (int i = 0; i < Model.Rows.Count; i++)
    {   <tr>
            <td> @count</td>
            <td>@Model.Rows[i][6]</td>
            <td>@Model.Rows[i][5]</td>
            <td>@Model.Rows[i][4]</td>
            <td>@Model.Rows[i][3]</td>
             <td><a href="@Url.Action("EditDeposit","Admin", new { @id =@Model.Rows[i][0]})">Edit</a></td>
            @*<td><a href="/MyAdmin/Edit/@(Model.Rows[i][0])">Edit</a></td>*@
           @*  <td><a href="~/Admin/EditDeposit/@(Model.Rows[i][0])">Edit</a></td>*@
        </tr>
        count++;
    }

</table>
